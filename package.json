    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.16",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
